package uvvlibrary;
//clase aluno!
public class Aluno {
    //Atributos da classe Aluno
    int idAluno;
    String nome, curso;
  //Declaração de construtor para a classe aluno: 
  //Um construtor é um método  especial em uma classe que é usado para 
  //inicializar objetos dessa classe quando eles são instanciados.
    public Aluno(int idAluno, String nome, String curso){
        this.curso = curso;
        this.idAluno = idAluno;
        this.nome = nome;
    }
}
